{
    "name": "slowlyo/slow-admin",
    "description": "采用 laravel、amis、vue3 打造的后台框架，拥有 120+ 种组件，只用后端代码就能实现复杂的页面效果，学习成本低，让开发者能够快速构建高效的后台管理系统。",
    "license": "MIT",
    "authors": [
        {
            "name": "Slowlyo",
            "email": "yguren@qq.com.com",
            "homepage": "https://gitee.com/slowlyo"
        }
    ],
    "homepage": "https://gitee.com/slowlyo/slow-admin",
    "keywords": [
        "Laravel",
        "SlowAdmin",
        "Admin"
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "~9"
    },
    "autoload": {
        "psr-4": {
            "Slowlyo\\SlowAdmin\\": "src/"
        },
        "files": [
            "src/Libs/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slowlyo\\SlowAdmin\\SlowAdminServiceProvider"
            ],
            "aliases": {
                "SlowAdmin": "Slowlyo\\SlowAdmin\\Facades\\SlowAdmin"
            }
        }
    }
}
